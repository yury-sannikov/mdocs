            form#contact_form(name='contact_form', action='includes/sendmail.php', method='post')
              .row
                .col-sm-6
                  .form-group.mb-30
                    input#form_name.form-control(name='form_name', type='text', placeholder='Enter Name', required='')
                .col-sm-6
                  .form-group.mb-30
                    input#form_email.form-control.required.email(name='form_email', type='email', placeholder='Enter Email')
              .row
                .col-sm-6
                  .form-group.mb-30
                    input#form_subject.form-control.required(name='form_subject', type='text', placeholder='Enter Subject')
                .col-sm-6
                  .form-group.mb-30
                    input#form_phone.form-control(name='form_phone', type='text', placeholder='Enter Phone')
              .form-group.mb-30
                textarea#form_message.form-control.required(name='form_message', rows='5', placeholder='Enter Message')
              .form-group.mb-30
                input#form_botcheck.form-control(name='form_botcheck', type='hidden', value='')
                button.btn.btn-dark.btn-theme-colored.btn-flat(type='submit', data-loading-text='Please wait...') Send your message
            // Contact Form Validation
            script(type='text/javascript').
              $("#contact_form").validate({
              submitHandler: function(form) {
              var form_btn = $(form).find('button[type="submit"]');
              var form_result_div = '#form-result';
              $(form_result_div).remove();
              form_btn.before('<div id="form-result" class="alert alert-success" role="alert" style="display: none;"></div>');
              var form_btn_old_msg = form_btn.html();
              form_btn.html(form_btn.prop('disabled', true).data("loading-text"));
              $(form).ajaxSubmit({
              dataType:  'json',
              success: function(data) {
              if( data.status == 'true' ) {
              $(form).find('.form-control').val('');
              }
              form_btn.prop('disabled', false).html(form_btn_old_msg);
              $(form_result_div).html(data.message).fadeIn('slow');
              setTimeout(function(){ $(form_result_div).fadeOut('slow') }, 6000);
              }
              });
              }
              });
