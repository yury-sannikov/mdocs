#pp_booking
  //- Booking
  #book-online.modal(tabindex='-1', role='dialog', aria-hidden='true', aria-labelledby='bookAppointmentLabel')
    .modal-backdrop.in(style='height: 100%;')
    .modal-dialog
      .modal-content
        .modal-header
          button.close(type='button', data-dismiss='modal', aria-hidden='true') ×
          h4#bookAppointmentLabel.modal-title
            | Request Now 
            span.subtitle= practice.name
        
        .caldetails(style='display: block;')
          form.contact_form
            .modal-body
              .left
                .label Full name
                input.txt-input(placeholder='Full name', name='form_name', type='text', value='', required='true')
                .label Email address
                input.txt-input(placeholder='Email address', name='email', type='email', value='', required='true')
                .label Phone number
                input.txt-input(placeholder='Phone number', name='form_phone', type='tel', value='', required='true')
              
              .left
                .label New Patient?
                ul
                  li
                    input#new-patient-yes(name='form_name_new', type='radio', value='yes', checked='checked', required='true')
                    label(for='new-patient-yes') Yes
                  li
                    input#new-patient-no(name='form_name_new', type='radio', value='no', required='true')
                    label(for='new-patient-no') No
                .label Reason
                input.txt-input(placeholder='E.g. initial consultation, follow-up, ...', maxlength='150', name='form_reason', type='text', value='', form_name)
                .label Date of Birth
                input.txt-input(placeholder='MM/DD/YYYY', maxlength='10', name='form_dob', type='text', value='')
                
              .left
                .label Select date
                input#datetimepicker(type="text", style="visibility: hidden; height:1px", name="form_date")
                #calendar-block.visible
                  
                    
            .modal-footer
              button.btn-cancel(type='button', data-dismiss='modal') Cancel
              button.btn-book-now(type='submit', value='Request Now', data-loading-msg='Submitting...', data-success-msg='Success!', data-error-msg='Error! Try again.', data-form-type='book') Request Now
