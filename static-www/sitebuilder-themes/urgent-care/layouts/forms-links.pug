doctype html
html(lang="en")
  head
    include ../partials/header.pug
  body.PARALLAX
    include ../partials/navbar.pug
    .main-content(style='margin:10px;')
      .container
        section(style="padding-bottom: 60px")
          != data.htmlContent
          <div class="col-xs-4 call-to-action pt-10 pb-30 mt-60 border-1px bg-lighter text-center">
            each link, i in data.links
              if i != 0 && i % 3 == 0
                </div>
                <div class="call-to-action pt-10 pb-30 mt-60 border-1px bg-lighter text-center">
              .form-link-item
                  h3 #{link.text}
                  a.btn.btn-gray.btn-theme-colored.btn-lg(href=link.href, target="_blank") Go To Form
          </div>
    include ../partials/footer.pug
