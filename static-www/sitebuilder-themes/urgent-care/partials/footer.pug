footer#footer.footer.pb-0.bg-black-111
  .container.pb-20
    .row.multi-row-clearfix
      .col-sm-6.col-md-5
        .widget.dark
          img(alt='', src='/assets/images/logo.png')
      .col-sm-6.col-md-2
        .widget.dark
          h5.widget-title.line-bottom Links
          ul
            li 
              a(href='/') Home
            li 
              a(href='/content/about') About
            li 
              a(href='/content/services') Services
            li 
              a(href='/content/employment') Employment
            li
              a(href='/feedback') Feedback
            li 
              a(href='/contact') Contact

      if practice.city != 'Woodbridge'
        .col-sm-6.col-md-3
          .widget.dark
            h5.widget-title.line-bottom Forms
            .latest-posts
              article.post.media-post.clearfix.pb-0.mb-10
                h5.post-title.mt-0.mb-5
                  a(href='/forms-links/faa-forms') FAA Flight Physical Form
                h5.post-title.mt-0.mb-5
                  a(href='/forms-links/immigration-services/') Immigration Forms
                h5.post-title.mt-0.mb-5
                  a(href='/forms-links/medical-records-release-form') Medical Records Release Form
                h5.post-title.mt-0.mb-5
                  a(href='/forms-links/motor-vehicle-accident-form') Motor Vehicle Accident Form
                h5.post-title.mt-0.mb-5
                  a(href='/forms-links/notice-of-privacy-practices') Notice of Privacy Practices
                h5.post-title.mt-0.mb-5
                  a(href='/forms-links/registration-packet') Registration Packet
                h5.post-title.mt-0.mb-5
                  a(href='/forms-links/school-entrance-physical') School Entrance Physical
                h5.post-title.mt-0.mb-5
                  a(href='/forms-links/sports-physical') Sports Physical
                h5.post-title.mt-0.mb-5
                  a(href='/forms-links/workers-comp-forms') Workers Comp Forms
      else 
        .col-sm-6.col-md-3
          .widget.dark
            h5.widget-title.line-bottom Forms
            .latest-posts
              article.post.media-post.clearfix.pb-0.mb-10
                h5.post-title.mt-0.mb-5
                  a(href='/forms-links/forms') All Forms

      .col-sm-6.col-md-2
        .widget.dark
          h5.widget-title.line-bottom Quick Contact
          ul.list.list-border
            li
              a(href='tel:'+practice.phone)= practice.phone
            li
              a(href='mail:'+practice.email)= practice.email
            li
              a.lineheight-20(href='/contact')= practice.address  
              br
              a.lineheight-20(href='/contact')= practice.city
              |, 
              a.lineheight-20(href='/contact')= practice.state
              |  
              a.lineheight-20(href='/contact')= practice.zip
              
  .footer-nav.bg-black-222.p-20
    .row.text-center
      .col-md-12
        case practice.city
          when 'Gainesville'
            p.text-white.font-11.m-0 Copyright © 2016 {{practice.name}}. All Rights Reserved.
          when 'Fairfax'
            p.text-white.font-11.m-0 Copyright © 2016 {{practice.name}}. All Rights Reserved.
          when 'Woodbridge'
            p.text-white.font-11.m-0 Copyright © 2016 {{practice.name}}. All Rights Reserved.
a.scrollToTop(href='#')
  i.fa.fa-angle-up
            
script(src='/assets/js/custom.js')
