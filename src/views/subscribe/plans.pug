extends ../layouts/layout.pug

block head
  title Subscribe

block styles
  link(rel='stylesheet', href='/app/lib/animate.css/animate.css')

block content
  style.
    /*========= Pricing ==========*/
    .pricing {
      border: solid 1px;
      border-radius: 5px;
      padding: 20px 30px;
      text-align: center;
      margin-bottom: 30px;
    }

    .pricing__title {
      margin-bottom: 20px;
    }

    .pricing__price {
      margin: 30px 0 20px;
      font-size: 30px;
      font-weight: bold;
    }

    .pricing__content ul {
      padding-left: 0;
      list-style: none;
      margin-bottom: 20px;
    }

    .pricing--featured {
      border: 0;
    }
    /*  =========== PRICING =========== */
    .pricing {
      border-color: #e1e4e7;
    }

    .pricing .pricing__title {
      color: #00A3C8;
    }

    .pricing .pricing__price {
      color: #FF196F;
    }

    .pricing:hover {
      border-color: #c5cbd0;
    }

    .pricing--featured {
      background-color: #00A3C8;
    }

    .pricing--featured .pricing__title {
      color: #fff;
    }

    .pricing--featured .pricing__price {
      color: #fff;
    }

    .pricing--featured .pricing__content li {
      color: #fff;
    }
  
