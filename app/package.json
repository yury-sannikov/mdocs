    "type": "git",
    "url": ""
  },
  "scripts": {
    "start": "concurrently \"npm run build\" \"npm run start-dev\"",
    "startc9": "concurrently \"npm run build\" \"better-npm-run start-dev-c9\"",
    "apps-dev": "concurrently --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "build": "better-npm-run build",
    "start-dev": "better-npm-run start-dev",
    "start-prod": "better-npm-run start-prod",
    "watch-client": "better-npm-run watch-client",
    "test": "NODE_ENV=test mocha --reporter spec",
    "lubricate": "npm cache clean && npm install && bower cache clean && bower install && gulp clean && gulp build && npm run build",
    "lint": "eslint --ignore-pattern server.js ./src/apps/dashboard/",
    "lint-fix": "eslint --ignore-pattern server.js --fix ./src/apps/*",
    "validate": "npm ls",
    "postinstall": "cd ./shared_modules && ./install && cd ..",
    "mounts3": "s3fs mdocs-sitebuilder ${HOME}/s3/ -ouse_cache=${HOME}/s3cache -omax_stat_cache_size=50000 -oparallel_count=10"
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "DEBUG": "app:*",
        "PORT": 3030
      }
    },
    "start-dev": {
      "command": "node ./server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "DEBUG": "app:*",
        "PORT": 3030
      }
    },
    "start-dev-c9": {
      "command": "node ./server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "DEBUG": "app:*",
        "PORT": 8080
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "PORT": 3030
      }
    },
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
