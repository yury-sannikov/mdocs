// Header Navigation Right
ul.nav-header.pull-right
  li
    a.btn.btn-success(target="_blank", href=config.SITEBUILDER_PREIVEW_URL + '/__generate?f=1&d=1&r=%2F')
      i.fa.fa-paper-plane-o
      |  Preview &amp; Deploy
  li.dropdown(class=url=='/app/profile'?'active':undefined)
    button.btn.btn-default.btn-image(type='button', href='#', data-toggle='dropdown')
      img(src='/app/oneui/assets/img/avatars/avatar10.jpg', alt='Avatar')
      | #{user.displayName || user.nickname || user.id}&nbsp;
      b.caret
    ul.dropdown-menu
      li
        a(href='/app/profile')
          i.fa.fa-fw.fa-user
          | &nbsp;My Profile
      li
        a(href='/app/subscription')
          i.fa.fa-fw.fa-list
          | &nbsp;My Subscription
      // li
      //   a(href='/contact')
      //     i.fa.fa-fw.fa-envelope-o
      //     | &nbsp;Contact Us
      li.divider
      li
        a(href='/app/logout')
          i.fa.fa-fw.fa-sign-out
          | &nbsp;Logout
  //
    <li>
    <button class="btn btn-default" type="button" data-ng-click="oneui.settings.sideOverlayOpen = ! oneui.settings.sideOverlayOpen" data-js-blur>
    <i class="fa fa-tasks"></i>
    </button>
    </li>
// END Header Navigation Right
// Header Navigation Left
ul.nav-header.pull-left
  li.hidden-md.hidden-lg
    button.btn.btn-default(type='button', data-ng-click='oneui.settings.sidebarOpenXs = ! oneui.settings.sidebarOpenXs', data-js-blur='')
      i.fa.fa-navicon
  li.hidden-xs.hidden-sm
    button.btn.btn-default(type='button', data-ng-click='oneui.settings.sidebarMini = ! oneui.settings.sidebarMini', data-js-blur='')
      i.fa.fa-ellipsis-v
  li.visible-xs
    // Toggle class helper (data-js-toggle-class attribute is initialized in js/directives.js)
    //
      <button class="btn btn-default" type="button" data-js-toggle-class="{target: '.js-header-search', class: 'header-search-xs-visible'}" data-js-blur>
      <i class="fa fa-search"></i>
      </button>
// END Header Navigation Left
