// Header Navigation Right
ul.nav-header.pull-right
  li
    button.btn.btn-success(data-uib-tooltip='Use this button to send an invite for a review.', data-tooltip-placement='bottom', data-toggle='modal', data-target='#modal-popout', type='button')
      i.fa.fa-paper-plane-o
      |  Send Review Invite
  li.dropdown(class=url=='/app/profile'?'active':undefined)
    button.btn.btn-default.btn-image(type='button', href='#', data-toggle='dropdown')
      img(src='/app/oneui/assets/img/avatars/avatar10.jpg', alt='Avatar')
      | #{user.displayName || user.nickname || user.id}&nbsp;
      b.caret
    ul.dropdown-menu
      li
        a(href='/app/profile')
          i.fa.fa-fw.fa-user
          | &nbsp;My Profile
      li
        a(href='/app/subscription')
          i.fa.fa-fw.fa-list
          | &nbsp;My Subscription
      // li
      //   a(href='/contact')
      //     i.fa.fa-fw.fa-envelope-o
      //     | &nbsp;Contact Us
      li.divider
      li
        a(href='/app/logout')
          i.fa.fa-fw.fa-sign-out
          | &nbsp;Logout
  //
    <li>
    <button class="btn btn-default" type="button" data-ng-click="oneui.settings.sideOverlayOpen = ! oneui.settings.sideOverlayOpen" data-js-blur>
    <i class="fa fa-tasks"></i>
    </button>
    </li>
